{
    "name": "dynamic/silverstripe-elemental-carousel",
    "description": "A block to implement dynamic/silverstripe-carousel, a simple carousel for Silverstripe in Bootstrap.",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "require": {
        "dnadesign/silverstripe-elemental": "^5",
        "dynamic/silverstripe-carousel": "^2"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^3",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\Elements\\Carousel\\": "src/",
            "Dynamic\\Elements\\Carousel\\Tests\\": "tests/"
        }
    }
}
